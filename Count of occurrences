function countOccurrences(arr){
    let countMap={};

    for(let i=0; i<arr.length; i++){
        if(countMap[arr[i]]===undefined){
            countMap[arr[i]]=1;
        }
        else{
            countMap[arr[i]]= countMap[arr[i]]+1;
        }
    }

    return countMap;
}

let res = countOccurrences(['apple', 'banana', 'apple', 'orange', 'banana', 'apple']);
console.log(res);
